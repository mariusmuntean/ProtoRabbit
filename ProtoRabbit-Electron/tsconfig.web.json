{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/**/*",
    "src/preload/*.d.ts",
    "src/preload/ProtoRabbitDataStore.ts",
    "src/preload/ProtoRabbitSettings.ts",
    "src/preload/SubscriptionManager.ts",
    "src/shared/**/*",
  ],
  "exclude": [
    "./out"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    },
  }
}
